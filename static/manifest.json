{
  "manifest_version": 2,
  "name": "Camellia",
  "version": "2.0.0",
  "author": "terron",
  "homepage_url": "https://github.com/ssimplix/camellia",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com; object-src 'self'",
  "short_name": "Camellia",
  "minimum_chrome_version": "62",
  "omnibox": {
    "keyword": "cml"
  },

  "permissions": [
    "bookmarks",
    "storage",
    "notifications",
    "topSites",
    "sessions",
    "<all_urls>"
  ],

  "key": "",

  "chrome_url_overrides" : {
    "newtab": "/index.html"
  }
}
